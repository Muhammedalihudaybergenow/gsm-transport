{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gsm-transport-web/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gsm-transport-web/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gsm-transport-web",
  "projects": {
    "broker": {
      "type": "library",
      "root": "libs/broker",
      "entryFile": "index",
      "sourceRoot": "libs/broker/src",
      "compilerOptions": {
        "tsConfigPath": "libs/broker/tsconfig.lib.json"
      }
    },
    "cache": {
      "type": "library",
      "root": "libs/cache",
      "entryFile": "index",
      "sourceRoot": "libs/cache/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "gsm": {
      "type": "application",
      "root": "apps/gsm",
      "entryFile": "main",
      "sourceRoot": "apps/gsm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gsm/tsconfig.app.json"
      }
    },
    "gsm-transport-web": {
      "type": "application",
      "root": "apps/gsm-transport-web",
      "entryFile": "main",
      "sourceRoot": "apps/gsm-transport-web/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gsm-transport-web/tsconfig.app.json"
      }
    }
  }
}